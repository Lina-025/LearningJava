import java.util.*;
public class Main {
   
   public static void main(String[] args) {
      Scanner s=new Scanner(System.in);
      
      System.out.println("enter an integer to ger its factorial: ");
      
      int a;
      a=s.nextInt();
   
      long factorial=1;
   for (int i = 1; i <= a; i++) {
      factorial *= i; }
   System.out.println("the factorial is : "+factorial);

      
   }
   
}
