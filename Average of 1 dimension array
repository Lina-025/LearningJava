import java.lang.*;

public class Main{
   public static void main(String[] args){
         int[] A ={1,2,3,4,5};
         int sum = 0;
         
         for (int num : A){
            sum += num; 
         }
         double average = sum / (double) A.length;
         System.out.println("average is: "+average);
   }
}
