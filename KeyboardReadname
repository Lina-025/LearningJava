import java.lang.*;
import java.util.*;

class KeyboardReadname {
   
   public static void main(String[] args)
   { 
      Scanner s=new Scanner(System.in);
      
      System.out.println("Please inter your name : ");
      String name ;
   
      name= s.nextLine();
      
      System.out.println("Welcome " + name);
   }
}
