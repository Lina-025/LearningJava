import java.util.*;
public class Main {
   
   public static void main(String[] args) {
      Scanner s=new Scanner(System.in);
      
      System.out.println("enter an integer: ");
      
      int a;
      a=s.nextInt();
   
      int i=1;
      while (i <= 10) {
         System.out.println( a + "x" +i+ "=" +(a*i));
         i++;
      }
   }
   
   }
   
