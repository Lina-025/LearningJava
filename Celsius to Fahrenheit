import java.util.*;

public class Main {
   public static void main(String[] args) {
      
      Scanner s=new Scanner(System.in);
      
      System.out.println("Please enter the room temperture in C: ");
      int C,F;
      C=s.nextInt();
      F= C * (9/5) + 32;
      
      System.out.println("the temperture in F is : "+F);
   }
}
